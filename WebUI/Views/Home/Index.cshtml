<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; Charset=UTF-8">

    <link rel="stylesheet" type="text/css" href="~/Content/jquery-ui.css">
    <link rel="stylesheet" type="text/css" href="~/Content/jquery-ui.structure.css">
    <link rel="stylesheet" type="text/css" href="~/Content/jquery-ui.theme.css">


    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap.min.css">

    <link rel="stylesheet" type="text/css" href="~/Content/view/trk.css">

    <script type="text/javascript" src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery-ui.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>

    <script src="~/Scripts/view/common.js" type="text/javascript"></script>
    <script src="~/Scripts/view/trk.js" type="text/javascript"></script>

</head>
<body>



    <div id="content">
        <div id="trk-6" class="trk">
            <fieldset>
                <legend> Колонка №: 6 </legend>
                <div id="trk-6-0" class="trk-left">
                    <div id="trk-6-0-rfid" class="trk-rfid">
                        RFID-карта <button id="button-trk-6-0-rfid" data-trk="6" data-side="0" class="ui-button ui-widget ui-corner-all button-rfid button-rfid-not-active "></button><label id="label-trk-6-0-rfid" class="label-rfid">: Не вставлена</label>
                    </div>
                    <fieldset>
                        <legend> Пистолет №: 11 (A92) </legend>
                        <table class="table table-gun">
                            <tr>
                                <th>Связь</th>
                                <th>Снят</th>
                                <th>Всего, м3</th>
                                <th>Заданно, л</th>
                                <th>Выдано, л</th>
                            </tr>
                            <tr>
                                <td id="gun-11-online"></td>
                                <td id="gun-11-taken"></td>
                                <td><input id="gun-11-total_volume" type="number" disabled="disabled" class="ui-widget-content ui-corner-all input-gun-volume" /></td>
                                <td><input id="gun-11-volume_to_write" type="number" disabled="disabled" class="ui-widget-content ui-corner-all input-gun-volume" /></td>
                                <td><input id="gun-11-last_out_volume" type="number" disabled="disabled" class="ui-widget-content ui-corner-all input-gun-volume" /></td>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    <button id="button-gun-11-deliver" data-gun="11" class="ui-button ui-widget ui-corner-all button-deliver">Выдать...</button>
                                </td>
                                <td colspan="3">
                                    <div id="progressbar-gun-11" class="progressbar-deliver"><div id="progress-label-gun-11" class="progress-label">Заправка...</div></div>
                                </td>
                            </tr>
                        </table>
                    </fieldset>
                </div>
                <div id="trk-6-1" class="trk-right">
                    <div id="trk-6-1-rfid" class="trk-rfid">
                        RFID-карта <button id="button-trk-6-1-rfid" class="ui-button ui-widget ui-corner-all button-rfid button-rfid-not-active "></button><label id="label-trk-6-1-rfid" class="label-rfid">: Не вставлена</label>
                    </div>
                    <fieldset>
                        <legend> Пистолет №: 12 (A92) </legend>

                    </fieldset>
                </div>
            </fieldset>
        </div>
    </div>

    <div id="confirm-rfid-card" title="" style="display:none">
        <form id="form-rfid-card">
            <fieldset>
                <table id="table-confirm-rfid-card">
                    <tr>
                        <th><label for="id" id="label-number-cards"></label>id карты :</th>
                        <td><input id="id" name="id" type="text" disabled="disabled" class="confirm-input ui-widget-content ui-corner-all" /></td>
                    </tr>
                    <tr>
                        <th><label for="Number" id="label-number-cards"></label>Номер карты :</th>
                        <td><input id="Number" name="Number" type="text" disabled="disabled" class="confirm-input ui-widget-content ui-corner-all" /></td>
                    </tr>
                    <tr>
                        <th><label for="DriverName" id="label-drivername-cards">DriverName :</label></th>
                        <td><input id="DriverName" name="DriverName" type="text" disabled="disabled" class="confirm-input ui-widget-content ui-corner-all" /></td>
                    </tr>
                    <tr>
                        <th><label for="AutoNumber" id="label-autonumber-cards">Номер автомобиля :</label></th>
                        <td><input id="AutoNumber" name="AutoNumber" type="text" disabled="disabled" class="confirm-input ui-widget-content ui-corner-all" /></td>
                    </tr>
                    <tr>
                        <th><label for="Debitor" id="label-debitor-cards">Код дебитора :</label></th>
                        <td><input id="Debitor" name="Debitor" type="number" disabled="disabled" class="confirm-input ui-widget-content ui-corner-all" /></td>
                    </tr>
                    <tr>
                        <th><label for="Sn1" id="label-sn1-cards"></label>Код транспортного средства :</th>
                        <td><input id="Sn1" name="Sn1" type="text" disabled="disabled" class="confirm-input ui-widget-content ui-corner-all" /></td>
                    </tr>
                    @*<tr>
                            <th><label for="Sn2" id="label-sn2-cards"></label>Sn2 :</th>
                            <td><input id="Sn2" name="Sn1" type="text" disabled="disabled" class="confirm-input ui-widget-content ui-corner-all" /></td>
                        </tr>*@
                    <tr>
                        <th><label for="AutoModel" id="label-automodel-cards">Модель автомобиля :</label></th>
                        <td><input id="AutoModel" name="AutoModel" type="text" disabled="disabled" class="confirm-input ui-widget-content ui-corner-all" /></td>
                    </tr>
                    <tr>
                        <th><label for="Street" id="label-street-cards"></label>Автоколона :</th>
                        <td><input id="Street" name="Street" type="number" disabled="disabled" class="confirm-input ui-widget-content ui-corner-all" /></td>
                    </tr>
                    <tr>
                        <th><label for="House" id="label-house-cards"></label>House :</th>
                        <td><textarea id="House" name="House" type="text" disabled="disabled" class="ui-widget-content ui-corner-all"></textarea></td>
                    </tr>
                    <tr>
                        <th><label for="Owner" id="label-owner-cards">Owner :</label></th>
                        <td><input id="Owner" name="Owner" type="text" disabled="disabled" class="confirm-input ui-widget-content ui-corner-all" /></td>
                    </tr>
                    <tr>
                        <th><label for="Active" id="label-active-cards">Активация карты :</label></th>
                        <td><input id="Active" name="Active" type="checkbox" disabled="disabled" class="confirm-input ui-widget-content ui-corner-all" /></td>
                    </tr>
                </table>
                @*<input id="id-nodes" name="id-nodes" type="hidden" />
                    <p>
                        <span class="ui-icon ui-icon-alert" style="float:left; margin:12px 12px 20px 0;"></span>
                        <label id="delete-confirm-text" name="delete-confirm-text"></label>
                    </p>*@
                <!-- Allow form submission with keyboard without duplicating the dialog button -->
                <input type="submit" tabindex="-1" style="position:absolute; top:-1000px">
            </fieldset>
        </form>
    </div>

    <table class="tab-content">
        <tr>
            <th><label id="number" for="number-value">Текущее время :</label></th>
            <td><label id="date-value"></label></td>
        </tr>
        <tr>
            <th><label id="number" for="number-value">Номер карточки :</label></th>
            <td><label id="number-value"></label></td>
        </tr>
        <tr>
            <th><label id="autonumber" for="autonumber-value">Номер автомобиля :</label></th>
            <td><label id="autonumber-value"></label></td>
        </tr>
        <tr>
            <th><label id="current_volume" for="current_volume-value">current_volume :</label></th>
            <td><label id="current_volume-value"></label></td>
        </tr>
        <tr>
            <th><label id="density" for="density-value">density :</label></th>
            <td><label id="density-value"></label></td>
        </tr>
        <tr>
            <th><label id="last_out_volume" for="last_out_volume-value">last_out_volume :</label></th>
            <td><label id="last_out_volume-value"></label></td>
        </tr>
        <tr>
            <th><label id="online" for="online-value">online :</label></th>
            <td><label id="online-value"></label></td>
        </tr>
        <tr>
            <th><label id="passage" for="passage-value">passage :</label></th>
            <td><label id="passage-value"></label></td>
        </tr>
        <tr>
            <th><label id="price_to_write" for="price_to_write-value">price_to_write :</label></th>
            <td><label id="price_to_write-value"></label></td>
        </tr>
        <tr>
            <th><label id="start" for="start-value">start :</label></th>
            <td><label id="start-value"></label></td>
        </tr>
        <tr>
            <th><label id="state" for="state-value">state :</label></th>
            <td><label id="state-value"></label></td>
        </tr>
        <tr>
            <th><label id="stop" for="stop-value">stop :</label></th>
            <td><label id="stop-value"></label></td>
        </tr>
        <tr>
            <th><label id="taken" for="taken-value">taken :</label></th>
            <td><label id="taken-value"></label></td>
        </tr>
        <tr>
            <th><label id="total_volume" for="total_volume-value">total_volume :</label></th>
            <td><label id="total_volume-value"></label></td>
        </tr>
        <tr>
            <th><label id="Trk06_0_status" for="Trk06_0_status-value">Trk06_0_status :</label></th>
            <td><label id="Trk06_0_status-value"></label></td>
        </tr>
        <tr>
            <th><label id="volume_to_write" for="volume_to_write-value">volume_to_write :</label></th>
            <td><label id="volume_to_write-value"></label></td>
        </tr>
        <tr>
            <th><label id="write_price" for="write_price-value">write_price :</label></th>
            <td><label id="write_price-value"></label></td>
        </tr>
    </table>

</body>

</html>

