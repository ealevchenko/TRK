@{
    ViewBag.Title = "АЗС АМКР";
    ViewBag.section = "Заправочная ведомость";
}





@section styles {


    @Styles.Render("~/jquery-ui/css")
    @Styles.Render("~/DataTables/css")
    @Styles.Render("~/DateTime/css")
    @*<link rel="stylesheet" type="text/css" href="~/Content/bootstrap.min.css">*@
    <link rel="stylesheet" type="text/css" href="~/Content/report/report.css">
}

@section scripts {

    <script>
        $('body').addClass('img-header-fuel-list');
    </script>

    <script>
        var log_view = '@System.Configuration.ConfigurationManager.AppSettings["Log_view"]';
        var type_test = '@System.Configuration.ConfigurationManager.AppSettings["Type_test"]';
        var controlTRK_ban = '@System.Configuration.ConfigurationManager.AppSettings["ControlTRK_ban"]';
        var pollDIO = '@System.Configuration.ConfigurationManager.AppSettings["PollDIO"]';
        var tanks_one = '@System.Configuration.ConfigurationManager.AppSettings["Tanks_one"]';
        var ns_advance = '@System.Configuration.ConfigurationManager.AppSettings["NS_advance"]';

        var tanks_alarm_high = '@System.Configuration.ConfigurationManager.AppSettings["Tanks_alarm_high"]';
        var tanks_warning_high = '@System.Configuration.ConfigurationManager.AppSettings["Tanks_warning_high"]';
        var tanks_warning_low = '@System.Configuration.ConfigurationManager.AppSettings["Tanks_warning_low"]';
        var tanks_alarm_low = '@System.Configuration.ConfigurationManager.AppSettings["Tanks_alarm_low"]';
    </script>

    @Scripts.Render("~/bundles/Ajax")
    @Scripts.Render("~/bundles/jquery-ui")
    @Scripts.Render("~/bundles/DataTables")
    @Scripts.Render("~/bundles/DateTime")
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @*<script src="~/Scripts/jquery.cookie.js" type="text/javascript"></script>*@

    <script src="~/Scripts/view/common.js" type="text/javascript"></script>
    <script src="~/Scripts/view/report_fuel_list.js" type="text/javascript"></script>
}

<div id="report" class="report-narrow">
    <div id="table-panel">
    </div>
    <div id="tabs-reports">
        <ul>
            <li><a href="#tabs-report-1" id="link-tabs-report-1"></a></li>
            @*<li><a href="#tabs-report-2" id="link-tabs-report-2"></a></li>*@
        </ul>
        <div id="tabs-report-1">
            @*<table class='display compact' id="table-report" style="width:100%" cellpadding="0">*@
            <table class='display compact table-bordered fuel-list-table' id="table-report" style="width:100%" cellpadding="0">
                <tfoot>
                    <tr><th colspan="3" class="title-foot">ИТОГО A92: </th><td class="total-foot" id="a92-volume"></td><td class="total-foot" id="a92-mass"></td><th colspan="8"></th></tr>
                    <tr><th colspan="3" class="title-foot">ИТОГО A95: </th><td class="total-foot" id="a95-volume"></td><td class="total-foot" id="a95-mass"></td><th colspan="8"></th></tr>
                    <tr><th colspan="3" class="title-foot">ИТОГО ДТ: </th><td class="total-foot" id="dt-volume"></td><td class="total-foot" id="dt-mass"></td><th colspan="8"></th></tr>
                    <tr><th colspan="3" class="title-foot">ИТОГО Керосин: </th><td class="total-foot" id="kerosin-volume"></td><td class="total-foot" id="kerosin-mass"></td><th colspan="8"></th></tr>
                </tfoot>
            </table>
        </div>
    </div>

</div>




